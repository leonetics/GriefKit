{
  "schemaVersion": 1,
  "id": "griefkit",
  "version": "${version}",
  "name": "GriefKit",
  "description": "Suite of utilities for griefing, developed with 2b2t in mind.",
  "authors": [
    "Leonetic"
  ],
  "contact": {
    "sources": "https://discord.gg/HAQ6CnqGpE"
  },
  "icon": "assets/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.griefkit.GriefKit"
    ]
  },
  "mixins": [
    "griefkit.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "200,45,0"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}", "1.21.7", "1.21.6"],
    "meteor-client": "*"
  }
}
